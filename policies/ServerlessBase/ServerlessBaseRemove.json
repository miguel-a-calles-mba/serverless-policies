{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ServerlessBase1",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteBucketPolicy",
                "s3:DeleteObject",
                "s3:DeleteBucket"
            ],
            "Resource": "*"
        },
        {
            "Sid": "ServerlessBase2",
            "Effect": "Allow",
            "Action": "cloudformation:DeleteStack",
            "Resource": "arn:aws:cloudformation:{{region}}:{{accountId}}:stack/*/*"
        }
    ]
}
